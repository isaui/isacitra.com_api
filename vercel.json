{
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/articles/(.*)",
        "dest": "/articles/$1",
        "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE"
      ]
      },
      {
        "src": "/authentication/(.*)",
        "dest": "/authentication/$1",
        "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE"
      ]
      },
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "OPTIONS",
        "DELETE"
      ]
      }
    ]
  }
