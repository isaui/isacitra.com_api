{
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/articles/(.*)",
        "dest": "/articles/$1",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://isacitra.com"
      }
      },
      {
        "src": "/authentication/(.*)",
        "dest": "/authentication/$1",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://isacitra.com"
      }
      },
      {
        "src": "/(.*)",
        "dest": "index.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://isacitra.com"
      }
      }
    ]
  }
