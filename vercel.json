{
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "source": "/articles/(.*)",
        "destination": "/articles/$1"
      },
      {
        "source": "/authentication/(.*)",
        "destination": "/authentication/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }